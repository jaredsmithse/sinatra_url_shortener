<div class="container">
  <form action="/urls" method="post">
    <p>Full URL</p>
    <input type="text" name="full_url">
    <input type="submit">
  </form>
  <p>Short URL's</p>
  <ul>
    <% @urls.each do |url| %>
      <li class="full-url"><a href=<%= "/" + url.short_url %>> <%= url.full_url %> </a></li>
      <li class="short-url"> <a href=<%= "/" + url.short_url %>> <%= url.short_url %> </a></li>
      <li class="click_count"> <%= url.click_count %> </li>
    <% end %>
  </ul>
</div>
