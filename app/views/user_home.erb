<div class="container">
  <h1>Welcome <%= @current_user.first_name %>!</h1>
  <form action="/urls" method="post">
    <p>Full URL</p>
    <input type="text" name="full_url">
    <button type="submit">Generate!</button>
  </form>
  <p>Short URL's</p>
  <% @urls.each do |url| %>
  <ul class="url">
    <% if @current_user.id == url.user_id %>
    <li class="full-url"> <%= url.full_url %> </li>
    <li class="short-url"> <a href=<%= "/url/" + url.short_url %>> <%= url.short_url %> </a></li>
    <li class="click-count"> <%= url.click_count %> </li>
  </ul>
  <% end %>
  <% end %>
</div>
